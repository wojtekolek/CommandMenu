{
  "name": "cmdmenu",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup ./index.ts --format esm,cjs --external react",
    "dev": "tsup ./index.ts --format esm,cjs --watch --external react",
    "lint": "eslint *.ts*",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --coverage --coverageReporters=html text-summary",
    "test:watch": "jest --watch --bail"
  },
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "babel-jest": "^29.0.1",
    "eslint": "^7.32.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "react": "18.2.0",
    "styled-components": "^5.3.5",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  }
}
